{
  "name": "@panyam/tsutils-gae",
  "version": "0.0.25",
  "author": "Sriram Panyam",
  "description": "Set of utilities for GAE",
  "homepage": "https://github.com/panyam/tsutils-gae#readme",
  "license": "ISC",
  "types": "lib/index.d.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panyam/tsutils-gae.git"
  },
  "keywords": [
    "google app engine"
  ],
  "bugs": {
    "url": "https://github.com/panyam/tsutils-gae/issues"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf dist && jest --clearCache && rm -rf static/dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "test": "jest --coverage --runInBand",
    "start": "webpack-dev-server --mode=development",
    "lint": "eslint './src' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint 'src/**/*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run build && npm run lint",
    "version": "npm run lintfix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@google-cloud/datastore": "^7.0.0",
    "@google-cloud/storage": "^6.7.0",
    "@panyam/tsutils": "*",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.13",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.1.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.8.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.2",
    "jest": "^29.2.2",
    "jest-each": "^29.2.1",
    "jest-mock": "^29.2.2",
    "jsdom": "^20.0.2",
    "json5": "^2.2.1",
    "node-mocks-http": "^1.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
