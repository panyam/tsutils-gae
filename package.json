{
  "name": "@panyam/tsutils-gae",
  "version": "0.0.4",
  "author": "Sriram Panyam",
  "description": "Set of utilities for GAE",
  "homepage": "https://github.com/panyam/tsutils-gae#readme",
  "license": "ISC",
  "types": "lib/index.d.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panyam/tsutils-gae.git"
  },
  "keywords": [
    "google app engine"
  ],
  "bugs": {
    "url": "https://github.com/panyam/tsutils-gae/issues"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf dist && jest --clearCache && rm -rf static/dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "test": "jest --coverage --runInBand",
    "start": "webpack-dev-server --mode=development",
    "lint": "eslint './src' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint 'src/**/*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run build && npm run lint",
    "version": "npm run lintfix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@google-cloud/connect-datastore": "^5.0.2",
    "@google-cloud/datastore": "^6.3.1",
    "@google-cloud/storage": "^5.7.4",
    "@handlebars/allow-prototype-access": "^1.0.3",
    "@panyam/tsutils": "0.0.*",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/handlebars": "^4.1.0",
    "@types/http-errors": "^1.8.0",
    "@types/markdown-it": "^12.0.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.26",
    "@typescript-eslint/parser": "^4.15.0",
    "ace-builds": "^1.4.12",
    "axios": "^0.21.1",
    "behave": "^0.2.2",
    "commander": "^7.0.0",
    "cookie-parser": "^1.4.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "js-yaml": "^4.0.0",
    "markdown-it": "^12.0.4",
    "morgan": "~1.10.0",
    "parse-duration": "^0.4.4",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.2",
    "uuid": "^8.3.2",
    "uuid-v4-validator": "^2.0.0",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/jsdom": "^16.2.6",
    "@types/passport": "^1.0.6",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/request": "^2.48.5",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "autoprefixer": "^10.2.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-mock": "^26.6.2",
    "jsdom": "^16.4.0",
    "json5": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
